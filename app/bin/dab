#!/bin/sh
# vim: ft=sh ts=4 sw=4 sts=4 noet
set -eu

dir="$PWD"
cd "$DAB"
[ "${DAB_PROFILING:-false}" = 'false' ] || echo "[PROFILE] $(date '+%s.%N') [STRT] subcommander $*"
subcommander "$@"
[ "${DAB_PROFILING:-false}" = 'false' ] || echo "[PROFILE] $(date '+%s.%N') [STOP] subcommander $*"
cd "$dir"
